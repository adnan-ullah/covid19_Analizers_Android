<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19analizes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUp"></activity>
        <activity android:name=".LoginForm"></activity>
        <activity android:name=".StartingActivity" />
        <activity android:name=".TrackingActivity" />
        <activity android:name=".each_BDState_data" />

        <activity android:name=".each_country_data" />
        <activity android:name=".div_update_activity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Splash_Activity"
            android:label="@string/title_activity_starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>